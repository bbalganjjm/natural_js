{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
//    "declarationMap": true,
//    "sourceMap": true,
//    "outFile": "./dist/natural.js",
//    "outDir": "./dist"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"],
  "typeAcquisition": {
    "enable": true,
    "include": ["jquery"]
  }
}
